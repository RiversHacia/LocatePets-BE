{
  "name": "slapjs-com",
  "version": "1.0.0",
  "main": "server.js",
  "author": "Christopher Hacia",
  "description": "A robust and agile boilerplate for rapid web development using Node.js, Express.js, React.js, MySQL, Bootstrap, SASS, and more.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/slapjs.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "react",
    "mysql",
    "bootstrap",
    "sass"
  ],
  "scripts": {
    "start": "node app.js",
    "dev:backend": "nodemon app.js",
    "dev:frontend": "webpack --watch",
    "styles:watch": "sass --watch shared/scss:shared/styles",
    "lint:js": "eslint .",
    "lint:css": "stylelint shared/scss/**/*.scss",
    "prepare:husky": "husky install",
    "lint-staged": "lint-staged",
    "start:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run styles:watch\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "email-validator": "^2.0.4",
    "express": "^4.19.2",
    "express-handlebars": "^7.1.2",
    "express-jwt": "^8.4.1",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "html-webpack-plugin": "^5.6.0",
    "jquery": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.3",
    "node-sass": "^9.0.0",
    "node-sass-middleware": "^1.1.0",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "sass": "^1.72.0",
    "sendgrid": "^5.2.3",
    "sequelize": "^6.37.2",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "xss-filters": "^1.2.7",
    "yamljs": "^0.3.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "supertest": "^6.3.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=18.10.0",
    "npm": ">=8.19.2"
  }
}
